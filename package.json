{
  "name": "kagyii-store",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "build": "npm-run-all clean transpile",
    "server": "node ./server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "author": "hein",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.757.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "google-auth-library": "^6.1.0",
    "jose": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "npm-run-all": "^4.1.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
